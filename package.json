{
  "name": "appcache-manifest",
  "version": "1.1.0",
  "description": "A CLI tool to generate HTML5 Application Cache Manifest.",
  "engines": {
    "node": ">=4"
  },
  "bin": {
    "appcache-manifest": "bin/appcache-manifest.js",
    "appcache-manifest-fixer": "bin/appcache-manifest-fixer.js"
  },
  "files": [
    "bin",
    "lib",
    "index.js"
  ],
  "scripts": {
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "coverage": "nyc report --reporter lcov && opener ./coverage/lcov-report/index.html",
    "clean": "rimraf .nyc_output coverage test-ws test-ws1 test-ws2",
    "lint": "eslint lib test index.js",
    "pretest": "npm run -s lint",
    "test": "nyc mocha \"test/*.js\" --compilers js:babel-register --timeout 30000",
    "watch": "mocha \"test/*.js\" --compilers js:babel-register --timeout 30000 --watch --growl",
    "codecov": "nyc report --reporter lcovonly && codecov"
  },
  "dependencies": {
    "chokidar": "^1.4.2",
    "glob-parent": "^3.1.0",
    "glob-stream": "^5.3.1",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "@types/node": "^4.0.30",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.18.0",
    "co": "^4.6.0",
    "codecov": "^1.0.1",
    "eslint": "^3.12.2",
    "eslint-config-mysticatea": "^7.0.1",
    "mocha": "^3.2.0",
    "nyc": "^10.0.0",
    "opener": "^1.4.2",
    "power-assert": "^1.2.0",
    "rimraf": "^2.5.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mysticatea/appcache-manifest.git"
  },
  "keywords": [
    "html5",
    "offline",
    "application",
    "cache",
    "manifest",
    "appcache",
    "generator",
    "cli",
    "tool",
    "commandline",
    "watch",
    "observe",
    "dir",
    "directory",
    "directories",
    "file",
    "files"
  ],
  "author": "Toru Nagashima",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mysticatea/appcache-manifest/issues"
  },
  "homepage": "https://github.com/mysticatea/appcache-manifest"
}
